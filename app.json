{
  "expo": {
    "name": "Dryfitech",
    "slug": "dry-fish-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dry-fitech@2x.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zentexe.dryfishapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dry-fitech@2x.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zentexe.dryfishapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/dry-fitech@2x.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "72ddd65b-4071-4220-b111-d03b2d3cfcdc"
      }
    }
  }
}
